{
  "name": "social-media-strategy-be",
  "version": "1.0.0",
  "description": "Labs22 SocialMediaProject",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=testing jest --watch --detectOpenHandles --runInBand",
    "start": "node index.js",
    "server": "nodemon",
    "server:c9": "export PORT=5000 && export APP_URL=https://$C9_HOSTNAME:3000 && nodemon index.js",
    "coverage": "jest --coverage --detectOpenHandles --forceExit",
    "migrate:testing": "knex migrate:latest --env testing"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@okta/jwt-verifier": "^1.0.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.13",
    "knex-cleaner": "^1.3.0",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.28",
    "morgan": "^1.10.0",
    "node-sass": "^7.0.0",
    "node-schedule": "^1.3.2",
    "pg": "^8.0.0",
    "query-string": "^6.11.1",
    "twit": "^2.2.11",
    "twitter-lite": "^0.9.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "jest": "^25.2.4",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/__tests__/**",
      "!**/coverage/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lambda-School-Labs/social-media-strategy-be.git"
  },
  "keywords": [],
